@model SistemaActas.UsuarioServices.Usuario

@{
    ViewBag.Title = "Detalles";
}

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <div class="form-horizontal">
        <h1 class="text-primary my-2">Usuarios</h1>
        <hr />
        <div class="form-group">
            <label>Nombre</label>
            <div class="col-md-10">
                @Html.TextBoxFor(model => model.nombre, new { @class = "form-control", @readonly = "readonly" })
            </div>
        </div>
        <div class="form-group">
            <label>Identificación</label>
            <div class="col-md-10">
                @Html.TextBoxFor(model => model.identificacion, new { @class = "form-control", @readonly = "readonly" })
            </div>
        </div>
        <div class="form-group">
            <label>Contraseña</label>
            <div class="col-md-10">
                @Html.TextBoxFor(model => model.contraseña, new { @class = "form-control", placeholder = "Password", Type = "password", @readonly = "readonly" })
            </div>
        </div>
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                @Html.ActionLink("Volver", "AllUsuario")
            </div>
        </div>
    </div>
}
